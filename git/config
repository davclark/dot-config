[diff "csv"]
    wordRegex = [^,\n]+[,\n]|[,]
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/clarkda/Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
    lg = log --oneline --graph --decorate --all
    ls = log --stat
    lo = log ORIG_HEAD.. --stat --no-merges
    undo = reset --hard
    prb = pull --rebase
    prq = pull-request
    cam = commit -am
    cm = commit -m
    rem = remote -v
    ff = pull --ff
    amend = commit --amend --no-edit
