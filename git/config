    default = simple
[color]
    ui = true
[diff "csv"]
    wordRegex = [^,\n]+[,\n]|[,]
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[alias]
    lg = log --oneline --graph --decorate --all
    ls = log --stat
    lo = log ORIG_HEAD.. --stat --no-merges
    undo = reset --hard
    prb = pull --rebase
    prq = pull-request
    cam = commit -am
    cm = commit -m
    rem = remote -v
    ff = pull --ff
    amend = commit --amend --no-edit
[svn]
    rmdir = true
